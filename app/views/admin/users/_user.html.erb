<tr>
  <th scope="row"><%= user.id %></th>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td><%= user.created_at %></td>
  <td>
    <%= link_to 'Edit', edit_admin_user_path(user), class: 'btn btn-secondary' %>
    <%= button_to "Delete", admin_user_path(user), class: 'btn btn-danger', method: :delete %>
  </td>
</tr>